<script type="text/javascript">
    apiready = function(){
        itemsJson = api.pageParam;
        // alert(JSON.stringify(itemsJson))
        document.getElementById('sendAuctionUnit').innerHTML = itemsJson.sendAuctionUnit;
        document.getElementById('name').innerHTML = itemsJson.name;
        document.getElementById('status').innerHTML = itemsJson.statusName;
        var speciaId = {};
        speciaId.esfid = itemsJson.id;
        new datalistModel(addDataToHtml,SpeciaItemsUrl,speciaId,10,5,100,true);

    }
    var allResult = {};
    function addDataToHtml(results,append) {
        alert(JSON.stringify(results))
        if (results && results.length > 0) {
            for(var i=0;i<results.length;i++){
                results[i].time = formatTimeToDate1(results[i].time)
                allResult[results[i].id]=results[i];
            }
          
            var data = {list: results};
            // alert(JSON.stringify(data))
            var html = template('scriptDiv1',data);
            if (append) {
                var div = document.createElement("div");
                div.innerHTML = html;
                document.getElementById('main').appendChild(div);
            } else {
                document.getElementById('main').innerHTML = html;
            }
        }
    }
    function openDetailsPage(id){
        openNewWindow('auction_detail','auction_detail.html',allResult[id])
    }
</script>
