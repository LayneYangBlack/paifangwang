            align-items: center;
            justify-content: center;
            border-top: 1px solid #ECECEC;
            border-right: 1px solid #ECECEC;
            float: left;
        }

        .nav_bottom li{
            width: 20%;
            height: 50px;
            font-size: 13px;
            float: left;
            display: flex;
            padding-top: 30px;
            justify-content: center;
        }
        .left_img{
            width: 40%;height: 100px; margin-left: 10px;
            float: left;
        }
        .right_msg{
            float: right;
            width: 55%;
            height: 90px;
            padding: 10px;
        }
        .msg_title{
            width: 100%;
            display: inline-block;
            height: 30px;
            line-height: 16px;
            font-size: 14px;
            overflow:hidden ;
            text-overflow: ellipsis;
            font-weight: 900;
            /*white-space: nowrap;*/
        }
        .price{
            display: block;
            height: 16px;
            line-height: 32px;
            color: red;
            font-size: 13px;
        }
        .price_num{
            font-size: 16px;
        }
        .msg_gray{
            height: 18px;
            line-height: 18px;
            display: block;
            color: #999999;
            font-size: 14px;
            margin-top: 20px;
        }
        .freechoose{
            width: 100%;
            height: 100px;
            display: flex;
            align-items: center;
            justify-content: space-around;
            word-spacing:8px;
        }
        .freechoose input{
            width:3.5rem;
            height: 28px;
            border: 1px solid #ebebeb;
            outline: none;
            text-align: center;
            line-height: 28px;

        }
        .freechoose span{
            color: #353535;
        }
        .freechoose button{
            width: 56px;
            height: 32px;
            background: #f54343;
            border-radius: 5px;
            color: #fff;
        }
        .topTitle1,.topTitle2,.topTitle3,.topTitle4{
            height: 3rem;
            width: 70%;
            overflow: hidden;
            text-align: center;
            white-space: nowrap;
            text-overflow: ellipsis;
        }
    </style>
