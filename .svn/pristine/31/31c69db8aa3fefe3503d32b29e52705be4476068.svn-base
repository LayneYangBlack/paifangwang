<script type="text/html" id="scriptDiv">
    <%for(var i=0;i < list.length; i++){%>
    <div class="item">
        <ul class="item_list">
            <li >
                <img class="left_img" onerror="this.src='../icon/default@1x.png' " src="<%=list[i].cover%>" alt="">
                <div class="right_msg">
                <span class="msg_title">
					<%=list[i].title%>
                </span>
               <span class="msg_gray">
                   <%if(list[i].handleWay ==1){%>
                         获取方式：<span >申请</span>
                   <%}else if(list[i].handleWay ==2){%>
                   获取方式：<span >付款</span>
                   <%}%>
               </span>
                </div>
            </li>


        </ul>
    </div>
    <%}%>

</script>
<script>

    //	document.getElementById("bg").addEventListener("touchmove",function(e){
    //		e.preventDefault();
    //	});
    var allResult={};
    var cityData={};
    var pageParm ={};
    var houseSourceTypes =$api.getStorage("houseSourceTypesList");//房屋资源类型
    var decorateTypes = $api.getStorage("decorateTypesList");//装饰类型
    //var ruleTypes =$api.getStorage("ruleTypesList");//规则类型
    var houseTypes =$api.getStorage("houseTypesList");//户型
    var buyTypes =$api.getStorage("buyTypesList");//购买类型
    var autionTypes =$api.getStorage("autionTypesList");//拍卖类型


    apiready = function() {

        new datalistModel(addDataToHtml, myServiceUrl,{}, 10, 5, 100, true);
    }
    function addDataToHtml(results,append) {
        if (results) {
            for (var i = 0; i < results.length; i++) {
                results[i].time = formatTimeToDay(results[i].time);
                allResult[results[i].id] = results[i];
            }
            var data = {list: results};
            var html = template('scriptDiv', data);
            if (append) {
                var div = document.createElement("div");
                div.innerHTML = html;
                document.getElementById('main').appendChild(div);
            } else {
                document.getElementById('main').innerHTML = html;
            }
        }

    }


</script>
