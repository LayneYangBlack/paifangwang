    <title>竞拍纪录</title>
    <link rel="stylesheet" href="../css/mui.css">
    <style>
        html{
            width: 100%;
            font-family: 黑体;

        }
        ul,li{
            margin: 0;
            padding: 0;
            list-style: none;
        }
        body{
            margin: 0;
            padding: 0;
            width: 100%;
            background-color: #EFEFEF;
        }
		.item_title{
			width: 100%;
			height: 40px;
			display: flex;
			align-items: center;
			background-color: #f6f6f6;
			justify-content: space-between;
			border-bottom: 1px solid #F2F2F2;
		}
		.status,.bidder{width: 20%;height: 100%;font-size: 13px;color: #999;}
		.price,.time{width: 30%;height: 100%;font-size: 13px;color: #999;}
		.status,.bidder{
			display: flex;
			align-items: center;
			justify-content: center;
		}
		.price,.time{
			display: flex;
			align-items: center;
			justify-content: flex-start;
		}
		.item{
			width: 100%;
			height: 40px;
			display: flex;
			align-items: center;
			background-color: #fff;
			justify-content: space-between;
			border-bottom: 1px solid #F2F2F2;
		}
		
		.first_logo{color: #fff;background-color: #F5A823;width: 34px;height: 19px;border-radius: 2px;line-height: 19px;text-align: center;}
		.first{color: #fb6462;}
		.out_logo{color: #999;background-color: #fff;width: 34px;height: 19px;border:1px solid #999;border-radius: 2px;line-height: 19px;text-align: center;}
    </style>
</head>
