            -ms-user-select:none;
            user-select:none;
            -webkit-tap-highlight-color: rgba(0,0,0,0);
            -webkit-tap-highlight-color:transparent;
        }
        html{
            width: 100%;
            font-family: 黑体;

        }
        ul,li{
            margin: 0;
            padding: 0;
            list-style: none;
        }
        body{
            margin: 0;
            padding: 0;
            width: 100%;
            background-color: #EFEFEF;
        }


        .anchor_list{
            width: 100%;
            background-color: #FFFFFF;
        }

        .content_title{
            list-style: none;
            width: 100%;
            height: 89px;

        }
        .content_title li{
            width: 33%;
            height:100%;
            text-align: center;
            border-right: 1px solid #ECECEC;
            float: left;
            font-size: 14px;
        }
        .item{
            /*margin-top: 10px;*/
            background-color: #FFFFFF;
            width: 100%;
            border-top: 1px solid #ECECEC;
        }
        .item_first{
            font-size: 16px;
            height: 49px;
            position: relative;
           line-height: 49px;
            padding-left: 20px;

        }
        .item_list{
            width: 100%;
            height:130px;
            border-top: 1px solid #ECECEC;
        }
        .item_list li{
            width:100%;
            height:130px;
            background-color: #FFFFFF;
            display: flex;
            align-items: center;
            /*justify-content: center;*/

            border-top: 1px solid #ECECEC;
            border-bottom: 1px solid #ECECEC;
        }
        .content_title,.item_list li:last-child{
            border-style: none;
        }

        .item_list2 li{
            width: 33%;
            height: 50px;
            font-size: 14px;
            color: #999999;
            display: -webkit-box;
            display: -moz-box;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-box-align: center;
            -moz-align-items: center;
            -webkit-align-items: center;
            align-items: center;
            -webkit-box-pack: center;
            -moz-justify-content: center;
            -webkit-justify-content: center;
            justify-content: center;
            border-top: 1px solid #ECECEC;
            border-right: 1px solid #ECECEC;
            float: left;
        }

        .item_list3 li{
            width: 49.6%;
            height: 50px;
            font-size: 14px;
            color: #999999;

            display: -webkit-flex;
            display: flex;
            display: -webkit-box;
            display: -moz-box;
            display: -ms-flexbox;

            -moz-align-items: center;
            -webkit-align-items: center;
            align-items: center;
            -webkit-box-align: center;

            -moz-justify-content: center;
            -webkit-justify-content: center;
            justify-content: center;
            -webkit-box-pack: center;
            border-top: 1px solid #ECECEC;
            border-right: 1px solid #ECECEC;
            float: left;
        }

        .nav_bottom li{
            width: 20%;
            height:50px;
            font-size:13px;
            float:left;

            display: -webkit-flex;
            display: flex;
            display: -webkit-box;
            display: -moz-box;
            display: -ms-flexbox;
            padding-top: 30px;

            -moz-justify-content: center;
            -webkit-justify-content: center;
            justify-content: center;
            -webkit-box-pack: center;
        }
        .right_msg{
            float: right;
            width: 55%;
            height: 100px;
            padding: 10px;
        }
        .msg_title{
            width: 100%;
            display: inline-block;
            max-height: 30px;
            line-height: 16px;
            font-size: 14px;
            overflow:hidden ;
            text-overflow: ellipsis;
            /*white-space: nowrap;*/
        }
        .price{
            display: block;
            height: 16px;
            line-height: 32px;
            color: red;
            font-size: 13px;
        }
        .price_num{
            font-size: 16px;
        }
        .msg_gray{
            height: 18px;
            margin-top: 20px;
            display: block;
            color: #999999;
            font-size: 14px;
        }
        .imggg{width:40%;height:100px; float: left;overflow: hidden;margin-left: 10px;}
    </style>
</head>
