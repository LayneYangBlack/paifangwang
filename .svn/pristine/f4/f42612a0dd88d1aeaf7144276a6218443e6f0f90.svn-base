    }
    function openEnterpriseHouseLive(){
        var liveListJson = {};
        liveListJson.willPlayUrl = liveEnterpriseHouseUrl;
        liveListJson.videoLiveListUrl =liveEnterpriseHouseBiddingPagerUrl;
        liveListJson.liveLoadUrl =liveEnterpriseHouseLoadUrl;
