        }
        #nav{
            width: 100%;
            height: 50px;
            background-color:#FCFCFC;
            position: fixed;
            bottom: 0;
        }
        .nav_bottom{
            width: 100%;
            height: 50px;
        }
        .nav_bottom li{
            width: 20%;
            height: 50px;
            font-size: 13px;
            float: left;
            display: flex;
            padding-top: 30px;
            justify-content: center;
        }
        .container{
            overflow: hidden;
            width: 75%;
            position: absolute;
            float: right;
            left: 25%;
            top: 0px;

        }
        .container li{height: 40px;line-height: 40px;font-size: 14px;padding-left: 10px;}
        .leftTitle{float: left;width: 25%;height: 20px;line-height: 20px;border-right:1px solid #000;margin-top:10px;}
