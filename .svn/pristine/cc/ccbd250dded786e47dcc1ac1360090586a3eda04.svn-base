<script>
	/*****************点击选择类型的时候   把选中的项  改变字体颜色*********/
	var types = document.getElementById("types");
	var liseleceta = types.getElementsByTagName("li");
	for (var i = 0;i < liseleceta.length;i++) {
		liseleceta[i].onclick=function(){
			for (var i = 0;i < liseleceta.length;i++) {
				if (liseleceta[i] == this) {
					liseleceta[i].style.color = "red";
				} else{
					liseleceta[i].style.color = "";
				}
			}
		}
	}; 

document.getElementById("bg_1").addEventListener("touchmove",function(e){
        e.preventDefault();
    });
	/************** 将页面的样式初始化******************/
	function reset(){
		document.getElementsByClassName("bg")[0].style.display = "none";
		document.getElementsByClassName("Brandul")[0].style.left = -450+"px";
		document.getElementsByClassName("Moreul")[0].style.left = -450+"px";
		if ($('.grade-eject').hasClass('grade-w-roll')){
	        $('.grade-eject').removeClass('grade-w-roll');
	    }
	}


/*************************点击所在地*************/
	document.getElementsByClassName("Brand")[0].onclick = function(){
		reset();
		document.getElementsByClassName("bg")[0].style.display = "block";
		var i = -450;
		var time = setInterval(function(){
			if(i >= 0){
				i = 0;
				clearInterval(time);
				return;
			}
			i+=75;
			document.getElementsByClassName("Brandul")[0].style.left = i+'px';
		},10)	
	}

/*****************点击类型**************/
	$(document).ready(function(){
	    $(".Regional").click(function(){
    		reset();
	    	document.getElementsByClassName("bg")[0].style.display = "block";
	        if ($('.grade-eject').hasClass('grade-w-roll')) {
	            $('.grade-eject').removeClass('grade-w-roll');
	        } else {
	            $('.grade-eject').addClass('grade-w-roll');
	        }
	    });
	});
	/****************点击类型**************/
	
	
	/****************点击状态**************/
	document.getElementsByClassName("more")[0].onclick = function(){
		reset();
		document.getElementsByClassName("bg")[0].style.display = "block";
		var i = -450;
		var time = setInterval(function(){
			if(i >= 0){
				i = 0;
				clearInterval(time);
				return;
			}
			i+=75;
			document.getElementsByClassName("Moreul")[0].style.left = i+'px';
		},10)			
	}
	/****************点击状态**************/
	
	
//	/****************点击选择 状态 里面的项  将选中的改变字体颜色***********/
	var typea = document.getElementsByClassName("Moreuls")[0];
	var liselecets = typea.getElementsByTagName("li");
	for (var i = 0;i < liselecets.length;i++) {
		liselecets[i].onclick=function(){
			for (var i = 0;i < liselecets.length;i++) {
				if (liselecets[i] == this) {
					liselecets[i].style.color = "red";
				} else{
					liselecets[i].style.color = "";
				}
			}
		}
	};

</script>
