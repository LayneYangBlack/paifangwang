    <div id="main"></div>
<script id="scriptDiv1" type="text/html">
    <% for (var i = 0; i < list.length; i ++) { %>
    <div class="collect_list">
        <div class="header">
            <img class="photo_head" src="<%=list[i].enterpriseCover%>" onerror="this.src='../icon/default@1x.png' "/>
                <span class="com_name">
                    <%=list[i].enterpriseName%>
                </span>
        </div>
        <div class="item-list">
            <span class="leftMsg">官方认证</span>
            <img class="nextIcon"  src="../icon/icon_right_normal.png">
        </div>

        <div class="item-list" onclick="openAllSpecia('<%=list[i].eid%>')">
        <span class="leftMsg">查看所有专场</span>
            <img class="nextIcon"  src="../icon/icon_right_normal.png">
        </div>

        <div class="item-list" onclick="openAllAuction('<%=list[i].eid%>')">
            <span class="leftMsg">查看所有拍品</span>
                <img class="nextIcon"  src="../icon/icon_right_normal.png"> 
        </div>
    </div>
    <div class="item" onclick="unFollow('<%=list[i].eid%>')">
        取消关注
    </div>
    <% } %> 
</script>
