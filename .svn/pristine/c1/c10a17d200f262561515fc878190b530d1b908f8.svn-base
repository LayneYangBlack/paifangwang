<script type="text/javascript">
	var flag = true;
	var hidJson = {};
	var hid ={};
	var piclist=[];
	var houseType =$api.getStorage("houseSourceTypes");
	var loadJson = {};
	apiready = function () {
	//	alert(JSON.stringify($api.getStorage("houseSourceTypes")))
		hidJson	= api.pageParam;
		hid.hid = hidJson.id;
	// alert(JSON.stringify(hidJson));
		ajaxGetWithProgress(hidJson.url,{hid:hidJson.id},function(ret,err){
			if(isNotBlack(ret)){
				if(isNotBlack(ret.shuffleFigure)){
						ret.piclist = JSON.parse(ret.shuffleFigure);
						for(var i in ret.piclist){
							piclist.push(ret.piclist[i]);
						}
						var UIScrollPicture = api.require('UIScrollPicture');
						UIScrollPicture.open({
						    rect: {
						        x: 0,
						        y: 30,
						        w:api.winWidth,
						        h:220
						    },
						    data: {
						        paths:piclist
						    },
						    styles: {
						        indicator: {
						            align: 'center',
						            color: '#FFFFFF',
						            activeColor: '#39bc30'
						        }
						    },
						    placeholderImg: '../icon/default@1x.png',
						    contentMode: 'scaleToFill',
						    interval: 5,
						    fixedOn: api.frameName,
						    loop: true,
						    fixed: false
						}, function(ret, err) {
						    if (ret) {
						    } else {
						        toast("获取图片失败")
						    }
						});
					}else{
						var UIScrollPicture = api.require('UIScrollPicture');
						UIScrollPicture.open({
							rect: {
								x: 0,
								y: 30,
								w:api.winWidth,
								h:220
							},
							data: {
								paths:['../icon/images.png']
							},
							styles: {
								indicator: {
									align: 'center',
									color: '#FFFFFF',
									activeColor: '#39bc30'
								}
							},
							placeholderImg: '../icon/default@1x.png',
							contentMode: 'scaleToFill',
							interval: 5,
							fixedOn: api.frameName,
							loop: true,
							fixed: false
						}, function(ret, err) {
							if (ret) {
							} else {
								toast("获取图片失败")
							}
						});
					}

				loadJson = ret;
				document.getElementById('addNum').setAttribute("data-numbox-step",ret.incrementPrice);
				if(ret.currentPrice == 0){
					document.getElementById('box').value = ret.reservePrice;
				}else{
					document.getElementById('box').value = ret.currentPrice;

				}
				if(ret.apply == true && ret.status == 2){
					document.getElementById('input_box').style.display = "flex";
					document.getElementById('box_bottom').style.display = "flex";
				}
				egid = ret.egid
				results = ret;
				var endTime = ret.endTime;
				ret.endTime = formatTimeToDate(ret.endTime);
				ret.reservePriceM = ret.reservePrice /10000;
				ret.currentPriceM = ret.currentPrice /10000;
				ret.evaluatePriceM = ret.evaluatePrice/10000;
				ret.typeNamn = houseType[ret.auctionType].name;

				var data = {list: ret};
				var html = template('scriptDiv1', data);
				document.getElementById('main').innerHTML = html;
				if(flag == true){
					var body = document.getElementsByTagName('body')[0];
					var scr = document.createElement('script');//创建一个script节点
					scr.setAttribute('src','../script/mui.min.js'); //给script节点添加路径
					body.appendChild(scr);
					flag = false;
				}else{
				}
			}else{alert(JSON.stringify(err))};
		});
	}
	//提交报名保证金验证有没有实名
	function openPayPages(){
		var promise = {};
		promise.hid = hidJson.id;
		promise.fee = document.getElementById('promise').innerHTML;
		promise.egid = loadJson.egid;
		promise.submitUrl = hidJson.submitUrl;
		user = getUserInfo();
		if(user.attestIdCard != 2){
			ajaxGetWithProgress(userLoadUrl,{},function(ret,err){
				if(isNotBlack(ret)){
					if(ret.attestIdCard != 2){
						dialogBox = api.require('dialogBox');
						showDialog('未实名认证','您的实名认证还没有通过，是否先去认证？','取消','去认证',function(){
							openNewWindow('auth','./auth.html')
						})
					}else{
						ret.fee = document.getElementById('promise').innerHTML;
						ret.hid = hidJson.id;
						ret.egid = loadJson.egid;
						ret.submitUrl = hidJson.submitUrl;

						openNewWindow('PayPages','./PayPages.html',ret);
					}
				}else{
					alert(JSON.stringify(err))
				};
			})
		}else{
			// alert(JSON.stringify(promise))
			openNewWindow('PayPages','./PayPages.html',promise);

		}
	}
	function startAlarm(){
		ajaxGetWithProgress(biteStartAlarmUrl,{hid:hid.hid},function(ret,err){
			if (ret&&ret.success){
				reloadHtml();
			}else {
				alert(JSON.stringify(err));
			}
