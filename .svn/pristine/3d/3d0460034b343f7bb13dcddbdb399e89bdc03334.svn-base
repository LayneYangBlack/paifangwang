<script type="text/javascript">
    var id;
    var params= {};
    apiready = function(){
        new datalistModel(addDataToHtml,liveWeekendBiddingPagerUrl,{twoLevelAreaId:1},10,5,100,true);
    }
    function openLiveDetail(id){
        params.hid =id;
        params.url =liveWeekendLoadUrl;
        openNewWindow('live_detail','./live_detail.html',params)
    }
    var allResult = {};
     function addDataToHtml(results) {
     //    alert(JSON.stringify(results))
        if (results && results.length > 0) {
            for(var i=0;i<results.length;i++){
                results[i].startTime = formatTimeToDate1(results[i].startTime)
                allResult[results[i].id]=results[i];
            }
            // alert(JSON.stringify(results))
            var data = {list: results};
            // alert(JSON.stringify(data))
            var html = template('scriptDiv1',data);
            document.getElementById('main').innerHTML = html;
        }
    }
</script>
