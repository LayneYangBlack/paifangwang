html {
	width: 100%;
	height: 100%;
}
ul,li{
    list-style: none;
    margin: 0;
    padding: 0;
}

body {
	background: #EFEFEF;
	width: 100%;
	height: 100%;
    margin:0;
    padding: 0;
    font-family:黑体;
}
#loadMoreDiv{text-align: center;font-size: 12px;color:#999;}
#topbar {
    background-color: #39bc30;
    height : 44px;
    line-height : 44px;
    color: #FFFFFF;
    font-size: 16px;
    position: relative;
/*    text-align: center;*/
    border-bottom: #E0E0E0 1px solid;
}

#leftButton{
    position: absolute;
    text-align: center;
   	margin-top: 2px;
    width: 45px;
    height: 42px;
    z-index: 10;
}
#leftButton img{
	width: 42px;
}


#title{
    /*width: 100%;*/
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin:0 60px;
    text-align: center;
    height: 43px;
}
.left_title{
	margin-left: 40px;
}
.right_img1{
	width: 20px;position: absolute;right: 3%;    top: 25%;
}
.right_img2{
	width: 20px;position: absolute;right: 13%;    top: 25%;
}
.right_img3{
	width: 20px;position: absolute;right: 23%;    top: 25%;
}
#rightButton{
    position: absolute;top: 15px;  right: 20px;
    line-height: 14px;
    font-size: 14px;
    color: #5B5B5B;
}
#rightButton1{
    height:20px;padding:5px;position: absolute;top: 6px;  right: 10px;
    line-height: 20px;
    font-size: 14px;
