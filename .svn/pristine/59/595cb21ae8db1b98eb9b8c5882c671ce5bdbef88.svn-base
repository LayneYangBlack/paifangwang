    overflow: hidden;
}
#rightButton2{
    height:20px;padding:5px;position: absolute;top: 6px;  right: 65px;
    line-height: 20px;
    font-size: 14px;
    background-color: #6BBDFA;

    color: #FFF;
    /*box-shadow: 0 0 2px 2px #DADDE0;*/
    border-radius: 5px;
    /*color: #6BBDFA;*/
}

