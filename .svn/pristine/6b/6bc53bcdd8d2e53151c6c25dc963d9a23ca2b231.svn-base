html{font-size:100px; outline:none; -webkit-text-size-adjust:none;}
body{font-family:""; width:100%; height:100%; color:#1f2228; background:#fff; padding:0px; margin:0px; -webkit-user-select:none; font-size:0.24rem;}
body, input, textarea, select, button, table{padding:0px; margin:0px; font-size:0.24rem;}
input{-webkit-appearance:none; -webkit-tap-highlight-color:rgba(0,0,0,0); -webkit-tap-highlight-color:transparent; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; -ms-box-sizing:border-box; box-sizing:border-box;}
a,button,input{outline:none; text-decoration:none;}
a{text-decoration:none; color:#1f2228; padding:0px; margin:0px; -webkit-tap-highlight-color:transparent;}
.l{float:left;}
.r{float:right;}
.hide{display:none;}
.mt05{margin-top:0.05rem;}
.mt10{margin-top:0.1rem;}
.mt_15{margin-top:0.15rem;}
.mt_20{margin-top:0.2rem;}
.mr_10{margin-right:0.1rem;}
.mr_20{margin-right:0.2rem;}
.ml_10{margin-left:0.1rem;}
.ml_20{margin-left:0.2rem;}
.pt_10{padding-top:0.1rem;}
.pb_10{padding-bottom:0.1rem;}
table{border-collapse:collapse; border-spacing:0;}
fieldset,
img,
iframe{border:0px;}
:focus{outline:none;}
address,
caption,
cite,
code,
dfn,
em,
strong,
th,
var{font-weight:normal; font-style:normal;}
ol,
ul{list-style:none; padding:0px; margin:0px;}
li{list-style:none; padding:0px; margin:0px;}
dl,dd,dt{padding:0px; margin:0px;}
p{padding:0px; margin:0px;}
caption,
th{text-align:left}
h1,h2,h3,h4,h5,h6{font-size:100%; font-weight:normal;}
blockquote:before,
blockquote:after,
q:before,
q:after{content:''}
abbr,
acronym{border:0px; font-variant:normal;}
sup{vertical-align:text-top;}
sub{vertical-align:text-bottom;}
select{appearance:none; -moz-appearance:none; -webkit-appearance:none;}
.clearfix:after{display:block; clear:both; content:'\20';}
textarea{resize:none;}
i{font-style:normal;}
em{font-style:normal;}
.pl_10{padding-left:0.1rem;}
.pr_10{padding-right:0.1rem;}
.p_10div{padding:0.1rem;}
.p_l_r10div{padding-left:0.1rem; padding-right:0.1rem; display:block;}
.p_l_r20div{padding-left:0.2rem; padding-right:0.2rem; display:block;}
.clear{padding:0px; margin:0px; clear:both;}
.dy_dd{overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.bt_cc{border-top:1px solid #dedede;}
.bb_cc{border-bottom:1px solid #dedede;}
.bj_fff{background:#fff;}
.red{color:#cf141b;}
.del_x{text-decoration:line-through;}
.wp680{width:6.4rem; margin:0px auto; padding:0px;}
.wp_100{width:100%; margin:0px auto; padding:0px;}
.div_bfb{width:100%; float:left;}