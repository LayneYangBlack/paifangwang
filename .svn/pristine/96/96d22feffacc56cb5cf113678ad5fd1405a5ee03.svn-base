    <title>所有拍品</title>
    <link rel="stylesheet" href="../css/mui.css">
    <style>
        html{
            width: 100%;
            font-family: 黑体;

        }
        ul,li{
            margin: 0;
            padding: 0;
            list-style: none;
        }
        body{
            margin: 0;
            padding: 0;
            width: 100%;
            background-color: #EFEFEF;
        }


        .anchor_list{
            width: 100%;
            background-color: #FFFFFF;
        }

        .content_title{
            list-style: none;
            width: 100%;
            height: 100px;

        }
        .content_title li{
            width: 33%;
            text-align: center;
            padding: 3% 0;
            border-right: 1px solid #ECECEC;
            float: left;
            font-size: 14px;
        }
        .item{
            /*margin-top: 10px;*/
            background-color: #FFFFFF;
            width: 100%;
            border-top: 1px solid #ECECEC;
        }
        .item:first-child{margin-top: 10px;}
        .item_first{
            font-size: 16px;
            height: 49px;
            position: relative;
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding-left: 20px;
             
        }
        .item_list{
            width: 100%;
            
            border-top: 1px solid #ECECEC;
        }
        .item_list li{
            width:100%;
            height:130px;
            background-color: #FFFFFF;
            display: flex;
            align-items: center;
            /*justify-content: center;*/

            border-top: 1px solid #ECECEC;
            border-bottom: 1px solid #ECECEC;
        }
        .content_title,.item_list li:last-child{
            border-style: none;
        }

        .item_list2 li{
            width: 33%;
            height: 50px;
            font-size: 14px;
            color: #999999;
            display: flex;
            align-items: center;
            justify-content: center;
            border-top: 1px solid #ECECEC;
            border-right: 1px solid #ECECEC;
            float: left;
        }

        .item_list3 li{
            width: 49.6%;
            height: 50px;
            font-size: 14px;
            color: #999999;
            display: flex;
            align-items: center;
            justify-content: center;
            border-top: 1px solid #ECECEC;
            border-right: 1px solid #ECECEC;
            float: left;
        }

        .nav_bottom li{
            width: 20%;
            height: 50px;
            font-size: 13px;
            float: left;
            display: flex;
            padding-top: 30px;
            justify-content: center;
        }
        .left_img{
            width: 40%;height: 100px; margin-left: 10px;
            float: left;
        }
        .right_msg{
            float: right;
            width: 55%;
            height: 100px;
            padding: 10px;
        }
        .msg_title{
            width: 100%;
            display: inline-block;
             height: 30px;
           line-height: 16px;
            font-size: 14px;
            overflow:hidden ;
            text-overflow: ellipsis;
            /*white-space: nowrap;*/
        }
        .price{
            display: block;
            height: 16px;
            line-height: 16px;
            color: red;
            font-size: 14px;
        }
        .price_num{
            font-size: 16px;
        }
        .msg_gray{
            height: 18px;
            line-height: 18px;
            display: block;
            color: #999999;
            font-size: 14px;
        }
        .yello_price{color: #f6aa2d;    float: right;}
    </style>
