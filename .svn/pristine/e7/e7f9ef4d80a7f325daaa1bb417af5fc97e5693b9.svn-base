<script type="text/html" id="scriptDiv">
	<div class="header">
		<div class="head">
			<div class="list_box">
				<span class="plot">
					<%=list.name%>
				<%if(list.type =1){%>
					<span class="logo house_logo">房源</span>
				<%}else if(list.type =2){%>
					<span class="logo car_logo">车位</span>
				<%}else if(list.type =3){%>
					<span class="logo shop_logo">商铺</span>
				<%}else if(list.type =4){%>
					<span class="logo store_logo">车位</span>
				<%}
				%>
				</span>
			</div>
		</div>
		<div class="color_example">
			<li><span class="square white"></span>可售</li>
			<li><span class="square green"></span>已选购</li>
			<li><span class="square red"></span>已流拍</li>
		</div>
	</div>
</script>
<script type="text/html" id="tableList">
	<table  border="1" style="margin-top: 1px;">
		<thead>
		<tr>
			<th><img src="../icon/tableHead.png" alt="" style="width: 45px;margin-top: 10px;"></th>
			<%for(var i=1;i<= list.maxRoomSize ; i++){%>
			<th ><%=i%></th>
			<%}%>
			<!--<th >1</th><th >2</th><th>3</th><th>4</th>-->
		</tr>
		</thead>
		<tbody class="tb">
		<%for(var j = 0;j< list.floors.length;j++){%>
			<tr>
				<td><%=list.floors[j].floor%></td>
				<%for(var z=0;z< list.floors[j].rooms.length;z++ ){%>
					<%if(list.floors[j].rooms[z].status ==0){%>
						<td class="type_three" onclick="openHouseDetail('<%=list.floors[j].rooms[z].hid%>')"><%=list.floors[j].rooms[z].roomNum%></td>
					<%}else if(list.floors[j].rooms[z].status ==1){%>
						<td class="type_two" ><%=list.floors[j].rooms[z].roomNum%></td>
				<%}else if(list.floors[j].rooms[z].status ==2){%>
						<td class="type_one" onclick="openHouseDetail('<%=list.floors[j].rooms[z].hid%>')"><%=list.floors[j].rooms[z].roomNum%></td>
				<%}%>
				<%}%>
			</tr>

		<%}%>
		</tbody>
	</table>
</script>
<script type="text/javascript">
	var window_width;
	var result={};
	var unit ={};
	var datas={};
    apiready = function(){
        window_width = api.winWidth;
        datas = api.pageParam;
		var data = {list: datas};
		var html = template('scriptDiv',data);
        var html1 = template('tableList',data);
		document.getElementById('main').innerHTML = html;
		document.getElementById('main1').innerHTML = html1;
    };
    function openHouseDetail(id) {
        var obj={};
        obj.id = id;
        obj.url=enterpriseESelectHouseLoadUrl;
        obj.bidPagerUrl = enterpriseESelectHouseBidPagerUrl;
        obj.bidUrl = enterpriseESelectHouseBidUrl;
        obj.commentUrl = enterpriseESelectHouseCommentUrl;
        obj.commentPager = enterpriseESelectHouseCommentPagerUrl;
        obj.commentReply = enterpriseESelectHouseReplyUrl;
        obj.start = enterpriseESelectHouseStartUrl;
        obj.cancelstart = enterpriseESelectHouseCancelStartUrl;
        obj.end = enterpriseESelectHouseEndUrl;
       	obj.cancelend = enterpriseESelectHouseCancelEndUrl;
       	obj.submitUrl=enterpriseESelectHouseSubmitGoldUrl;
        openNewWindow('e_room_detail','./e_room_detail.html',obj);
    }
    function reload() {
        location.reload()
    }
</script>
